{
  "name": "jovan/verso",
  "license": "proprietary",
  "description": "Test case for Verso",
  "type": "project",
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "symfony/config": "^6.2",
    "symfony/console": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/framework-bundle": "^7.0",
    "symfony/http-foundation": "^6.2",
    "symfony/http-kernel": "^6.4",
    "symfony/yaml": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.2.*"
    },
    "runtime": {
      "class": "Symfony\\Component\\Runtime\\GenericRuntime"
    }
  }
}
